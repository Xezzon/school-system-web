    "license": "Apache-2.0",
    "sideEffects": [
        "dist/*",
        "*.less"
    ],
    "scripts": {
        "docker:build": "docker build -t school-system-web:$npm_package_version .",
        "lint": "npm run lint:style && npm run lint:script",
        "lint:style": "pnpx stylelint 'src/**/*.less' --syntax less",
        "lint:script": "pnpx eslint src --ext '.js,.jsx'",
        "lint-fix": "npm run lint-fix:style && npm run lint-fix:script",
        "lint-fix:style": "npm run lint:style -- --fix",
        "lint-fix:script": "npm run lint:script -- --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "webpack:analyze": "pnpx webpack --config .webpack/webpack.config.prod.js --progress=profile --analyze --no-stats",
        "webpack:build": "pnpx webpack --config .webpack/webpack.config.prod.js --progress",
        "webpack:watch": "pnpx webpack serve --config .webpack/webpack.config.dev.js --progress"
    },
    "dependencies": {
        "@ant-design/icons": "^4.6.2",
        "@ant-design/pro-layout": "^6.19.7",
        "antd": "^4.16.5",
        "axios": "^0.21.1",
        "bootstrap": "^5.0.2",
        "dayjs": "^1.10.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-react": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "@webpack-cli/serve": "^1.5.1",
        "antd-dayjs-webpack-plugin": "^1.0.6",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-jsx-control-statements": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^8.0.1",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^5.2.6",
        "css-minimizer-webpack-plugin": "^3.0.2",
        "eslint": "^7.29.0",
        "eslint-plugin-jsx-control-statements": "^2.2.1",
        "eslint-plugin-react": "^7.24.0",
        "html-webpack-plugin": "^5.3.2",
        "less": "^4.1.1",
        "less-loader": "^10.0.0",
        "mini-css-extract-plugin": "^1.6.1",
        "style-loader": "^3.0.0",
