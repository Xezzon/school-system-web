    "license": "Apache-2.0",
    "sideEffects": [
        "dist/*",
        "*.less"
    ],
    "scripts": {
        "docker:build": "docker build -t school-system-web:$npm_package_version .",
        "lint": "npm run lint:style && npm run lint:script",
        "lint:style": "npx stylelint 'src/**/*.less' --syntax less",
        "lint:script": "npx eslint src --ext '.js,.jsx'",
        "lint-fix": "npm run lint-fix:style && npm run lint-fix:script",
        "lint-fix:style": "npm run lint:style -- --fix",
        "lint-fix:script": "npm run lint:script -- --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "webpack:analyze": "npx webpack --config .webpack/webpack.config.prod.js --progress=profile --analyze --no-stats",
        "webpack:build": "npx webpack --config .webpack/webpack.config.prod.js",
        "webpack:watch": "npx webpack serve --config .webpack/webpack.config.dev.js"
    },
    "dependencies": {
        "antd": "^4.13.1",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "dayjs": "^1.10.4",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-transform-runtime": "^7.13.9",
        "@babel/preset-env": "^7.13.9",
        "@babel/preset-react": "^7.12.13",
        "@webpack-cli/serve": "^1.3.0",
        "antd-dayjs-webpack-plugin": "^1.0.6",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-jsx-control-statements": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^7.1.2",
        "copy-webpack-plugin": "^8.0.0",
        "css-loader": "^5.1.1",
        "css-minimizer-webpack-plugin": "^1.2.0",
        "eslint": "^7.21.0",
        "eslint-plugin-jsx-control-statements": "^2.2.1",
        "eslint-plugin-react": "^7.22.0",
        "html-webpack-plugin": "^5.2.0",
        "less": "^4.1.1",
        "less-loader": "^8.0.0",
        "mini-css-extract-plugin": "^1.3.9",
        "stylelint": "^13.11.0",
        "stylelint-config-recommended": "^3.0.0",
        "thread-loader": "^3.0.1",
